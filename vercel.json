{
    "version": 2,
    "framework": "laravel",
    "functions": {
      "api/index.php": {
        "runtime": "vercel-php@0.3.1"
      }
    },
    "routes": [{
      "src": "/(.*)",
      "dest": "/api/index.php"
    }],
    "env": {
      "APP_ENV": "production",
      "APP_DEBUG": "false",
      "APP_URL": "https://topedges.online",
  
      "CACHE_DRIVER": "array",
      "LOG_CHANNEL": "stderr",
      "SESSION_DRIVER": "cookie"
    }
  }
  